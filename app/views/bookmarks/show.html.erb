<section class="main-content">

  <%= render "layouts/errors" %>

  <%= render "users/header_nav", user: @user %>


  <section class="user-main-body group">
    <%= render "users/user_info", user: @user %>

    <section class="mid-content">
      <header class="">
        <h3 class="red-title"><%= @user.name %>'s Bookmarks</h3>
        <hr>
      </header>

      <% @user.bookmarked_businesses.each do |business| %>
        <%= render "businesses/search_result", business: business %>
      <% end %>
    </section>

    <section class="sidebar-right">


    </section>

  </section>
</section>