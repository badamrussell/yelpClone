<header>
  <nav>
    <%= current_user.email if current_user %>
    <br>
    <%= session[:session_token] %>
    <br>
    <% if signed_in? %>
      <%= current_user.email %>
      <%= button_to "sign out", session_url, method: :delete %>
    <% else %>
      <%= link_to "log_in", new_session_url %>
      <%= link_to "sign up", new_user_url %>
    <% end %>
  </nav>
</header>
<hr>