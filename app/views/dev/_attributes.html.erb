<% obj.attributes.each do |key, value| %>
  <% next if key.include?("id") %>
  <% next if key == "created_at" %>
  <% next if key == "updated_at" %>

  <% if is_form %>
    <label for="<%= prefix %>_<%= key %>"><%= key %></label>
    <br>
    <input type="text" name="<%= prefix %>[<%= key %>]" value="<%= value %>" id="<%= prefix %>_<%= key %>">
    <br>
  <% else %>
  <div>
    <h4><%= key %></h4>
    <p><%= value %></p>
  </div>
  <% end %>
<% end %>